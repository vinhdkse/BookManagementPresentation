    <script>$(function () { CompAnimations.init(); });</script>
    <script src="~/FEUser/js/pages/jquery.mask.js"></script>
    <script>
        function readURL(input) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();

                reader.onload = function (e) {
                    $("#img").attr("src", e.target.result);
                };
                document.getElementById("checkChange").value = "YES";
                reader.readAsDataURL(input.files[0]);
            }
        }
        $("#val_img").change(function () {
            readURL(this);
        });
        $('#masked-card').mask('9999-9999-9999-9999');
        $('#masked-cardnum').mask('9999-9999-9999');
        $('#masked-serial').mask('9999-9999-9999');

        $('#cvccart').mask('999');
        $('#history').hover(function () {
            document.getElementById('historyre').style.display = "block";
        });
        $('#history').mouseleave(function () {
            document.getElementById('historyre').style.display = "none";
        });
    </script>
    <script src="~/Scripts/jquery.signalR-2.4.0.min.js"></script>
